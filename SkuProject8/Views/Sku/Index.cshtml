@model IEnumerable<SkuProject8.Models.Sku>
@{
    Layout = null;
}

<form asp-controller="Students" asp-action="Index" method="post" enctype="multipart/form-data">
    <input type="file" name="file"/>
    <hr/>
    <button type="submit">Upload</button>
</form>

@if (Model.Count() > 0) 
{ 
    <hr/>
    <table cellpadding="0" cellspacing="0" border="1">
        <tr>
            <td>Id</td>
            <td>Sku_Id</td>
            <td>Comments</td>
        </tr>
       
            @foreach(SkuProject8.Models.Sku sku in Model)
                {
                    <tr>
                        <td>@sku.Id</td>
                        <td>@sku.Sku_Id</td>
                        <td>@sku.Comments</td>
                    </tr>
                }
        
    </table>
}
